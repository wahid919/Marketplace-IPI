{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": [
        "yii2",
        "framework",
        "basic",
        "project template"
    ],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.10",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "dmstr/yii2-adminlte-asset": "2.2.4",
        "schmunk42/yii2-giiant": "@stable",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "kartik-v/yii2-widget-select2": "dev-master",
        "kartik-v/yii2-widget-datepicker": "@dev",
        "2amigos/yii2-selectize-widget": "~1.0",
        "wbraganca/yii2-dynamicform": "dev-master",
        "phpoffice/phpexcel": "*",
        "kartik-v/yii2-widget-spinner": "@dev",
        "kartik-v/yii2-widget-datetimepicker": "dev-master",
        "drsdre/yii2-wizardwidget": "^1.2",
        "kartik-v/yii2-widget-activeform": "@dev",
        "kartik-v/yii2-money": "dev-master",
        "kartik-v/yii2-grid": "dev-master",
        "kartik-v/yii2-export": "dev-master",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-datecontrol": "dev-master",
        "kartik-v/yii2-widget-depdrop": "dev-master",
        "kartik-v/yii2-builder": "dev-master",
        "mdmsoft/yii2-admin": "~2.0",
        "endroid/qr-code": "^2.5",
        "nasution/terbilang": "^0.0.4",
        "phpoffice/phpspreadsheet": "^1.8",
        "midtrans/midtrans-php": "^2.5",
        "richardfan1126/yii2-js-register": "*",
        "himiklab/yii2-recaptcha-widget": "*",
        "kartik-v/yii2-widget-rating": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "platform": {
            "php": "7.0"
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    }
}
